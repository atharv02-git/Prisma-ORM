// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @default(autoincrement()) @id //Auto-increment allows a unique number to be generated automatically when a new record is inserted into a table
  username String @unique
  password String
}

